<%- include ('./components/header')%>
   <form method="post" action =/edit-post/<%= posts[i]._id%>>
      <div class="form-group" >
        <label for="name" >Title</label>
        <input type="text" name="title"  value =<%=result.title%> class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter title">
      </div>
      <div class="form-group" class="card">
        <label for="message" >Desc</label>
        <input type="text" name="desc" value = <%= result.desc%> class="form-control" id="exampleInputPassword1" placeholder="Article">
      </div>
      <button type="submit" class="btn btn-primary">Submit</button>
  </form>